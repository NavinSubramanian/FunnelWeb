{
    "version": 2,
    "builds": [
    {
        "src": "./index.js",
        "use": "@vercel/node"
    }
    ],
    "routes": [
    {
        "src": "/(.*)",
        "dest": "/"
    }
    ],
    "headers": [
    {
      "source": "/css/(.*)", 
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/images/(.*)", 
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400" 
        }
      ]
    },
    {
      "source": "/(.*).html", 
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache" 
        }
      ]
    }
  ]
 }